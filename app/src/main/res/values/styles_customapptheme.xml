<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="EditTextCustomAppTheme" parent="android:Widget.EditText">
        <item name="android:background">@drawable/customapptheme_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="AutoCompleteTextViewCustomAppTheme" parent="android:Widget.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/customapptheme_list_selector_holo_light
        </item>
        <item name="android:background">@drawable/customapptheme_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="CheckBoxCustomAppTheme" parent="android:Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/customapptheme_btn_check_holo_light</item>
    </style>

    <style name="RadioButtonCustomAppTheme" parent="android:Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/customapptheme_btn_radio_holo_light</item>
    </style>

    <style name="ButtonCustomAppTheme" parent="android:Widget.Button">
        <item name="android:background">@drawable/customapptheme_btn_default_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ImageButtonCustomAppTheme" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/customapptheme_btn_default_holo_light</item>
    </style>

    <style name="SpinnerCustomAppTheme" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/customapptheme_spinner_background_holo_light
        </item>
        <item name="android:dropDownSelector">@drawable/customapptheme_list_selector_holo_light
        </item>
    </style>

    <style name="SpinnerDropDownItemCustomAppTheme" parent="android:Widget.DropDownItem.Spinner">
        <item name="android:checkMark">@drawable/customapptheme_btn_radio_holo_light</item>
    </style>

    <style name="TabCustomAppTheme">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        <item name="android:background">@drawable/customapptheme_tab_indicator_holo</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minWidth">80dip</item>
    </style>

    <style name="TabTextCustomAppTheme">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <!-- v14 <item name="android:textAllCaps">true</item> -->
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">2</item>
        <item name="android:maxWidth">180dip</item>
    </style>

    <style name="ProgressBarCustomAppTheme" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">
            @drawable/customapptheme_progress_horizontal_holo_light
        </item>
        <item name="android:indeterminateDrawable">
            @drawable/customapptheme_progress_indeterminate_horizontal_holo_light
        </item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>
    </style>

    <style name="SeekBarCustomAppTheme" parent="android:Widget.SeekBar">
        <item name="android:progressDrawable">
            @drawable/customapptheme_scrubber_progress_horizontal_holo_light
        </item>
        <item name="android:indeterminateDrawable">
            @drawable/customapptheme_scrubber_progress_horizontal_holo_light
        </item>
        <item name="android:minHeight">13dip</item>
        <item name="android:maxHeight">13dip</item>
        <item name="android:thumb">@drawable/customapptheme_scrubber_control_selector_holo_light
        </item>
        <item name="android:thumbOffset">16dip</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
    </style>

    <style name="RatingBarCustomAppTheme" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/customapptheme_ratingbar_full_holo_light
        </item>
        <item name="android:indeterminateDrawable">
            @drawable/customapptheme_ratingbar_full_holo_light
        </item>
    </style>

    <style name="RatingBarBigCustomAppTheme" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/customapptheme_ratingbar_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/customapptheme_ratingbar_holo_light
        </item>
        <item name="android:minHeight">35dip</item>
        <item name="android:maxHeight">35dip</item>
    </style>

    <style name="RatingBarSmallCustomAppTheme" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/customapptheme_ratingbar_small_holo_light
        </item>
        <item name="android:indeterminateDrawable">
            @drawable/customapptheme_ratingbar_small_holo_light
        </item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>
    </style>

    <style name="ToggleCustomAppTheme" parent="android:Widget.Button.Toggle">
        <item name="android:background">@drawable/customapptheme_btn_toggle_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ListViewCustomAppTheme" parent="android:Widget.ListView">
        <item name="android:listSelector">@drawable/customapptheme_list_selector_holo_light</item>
    </style>

    <style name="ListViewCustomAppTheme.White" parent="android:Widget.ListView.White">
        <item name="android:listSelector">@drawable/customapptheme_list_selector_holo_light</item>
    </style>

    <style name="SpinnerItemCustomAppTheme" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:textColor">#000000</item>
    </style>

</resources>